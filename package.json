    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "name": "final-project-template-turbo-vite",
  "packageManager": "npm@8.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "dependencies": {
    "@nivo/bar": "^0.84.0",
    "@nivo/core": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@nivo/pie": "^0.84.0",
